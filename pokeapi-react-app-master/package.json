{
  "name": "pokeapi-react-app",
  "version": "0.1.0",
  "author": "gagrison <gagrison@gmail.com>",
  "private": false,
  "dependencies": {
    "axios": "~0.26.0",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-redux": "~7.2.0",
    "react-router-dom": "~5.2.0",
    "react-scripts": "3.4.1",
    "redux": "~4.0.5",
    "redux-thunk": "~2.3.0",
    "reselect": "~4.0.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb"
    ],
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/prop-types": 0,
      "react/jsx-fragments": 0,
      "react/destructuring-assignment": [
        0
      ],
      "jsx-a11y/click-events-have-key-events": 0,
      "jsx-a11y/no-noninteractive-element-interactions": 0,
      "comma-dangle": 0,
      "space-before-function-paren": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "~18.2.0",
    "node-sass": "~4.14.1"
  }
}
